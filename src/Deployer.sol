pragma solidity >=0.8.0;

import { Test, console } from "forge-std/Test.sol";

import { INexus } from "./interfaces/INexus.sol";
import { INexusFactory } from "./interfaces/INexusFactory.sol";
import { INexusBootstrap } from "./interfaces/INexusBootstrap.sol";
import { IValidator } from "./interfaces/IValidator.sol";

import { DefaultValidator } from "./modules/DefaultValidator.sol";

contract NexusDeployer is Test {
    ////////////////////////////////////////////////////////////////
    //                    Deployment utilities                    //
    ////////////////////////////////////////////////////////////////

    struct NexusDeployment {
        INexus implementation;
        INexusFactory factory;
        INexusBootstrap bootstrap;
    }

    function deployNexusCreationCode(bytes memory creationBytecode) private returns (address contractAddress) {
        assembly {
            contractAddress := create(0, add(creationBytecode, 0x20), mload(creationBytecode))
        }
        require(contractAddress != address(0), "deployment failed");
    }

    function deployNexusImplementation(
        address entrypoint,
        address validator,
        bytes memory validatorOnInstallData
    )
        internal
        returns (INexus nexus)
    {
        bytes memory creationBytecode =
            bytes.concat(NEXUS_IMPLEMENTATION_V1_2_0, abi.encode(entrypoint, validator, validatorOnInstallData));
        nexus = INexus(deployNexusCreationCode(creationBytecode));
        vm.label(address(nexus), "NexusImplementation");
    }

    function deployNexusAccountFactory(
        address implementation,
        address owner
    )
        internal
        returns (INexusFactory factory)
    {
        bytes memory creationBytecode = bytes.concat(NEXUS_ACCOUNT_FACTORY, abi.encode(implementation, owner));
        factory = INexusFactory(deployNexusCreationCode(creationBytecode));
        vm.label(address(factory), "NexusAccountFactory");
    }

    function deployNexusBootstrap(
        address validator,
        bytes memory validatorOnInstallData
    )
        internal
        returns (INexusBootstrap bootstrap)
    {
        bytes memory creationBytecode =
            bytes.concat(NEXUS_BOOTSTRAP_V1_2_0, abi.encode(validator, validatorOnInstallData));
        bootstrap = INexusBootstrap(deployNexusCreationCode(creationBytecode));
        vm.label(address(bootstrap), "NexusBootstrap");
    }

    function deployNexusDefaultValidator() internal returns (IValidator validator) {
        validator = IValidator(address(new DefaultValidator()));
    }

    function deployNexus(address entrypoint, address owner) internal returns (NexusDeployment memory deployment) {
        IValidator validator = deployNexusDefaultValidator();
        INexus implementation = deployNexusImplementation(entrypoint, address(validator), "");
        INexusFactory factory = deployNexusAccountFactory(address(implementation), owner);
        INexusBootstrap bootstrap = deployNexusBootstrap(address(validator), "");

        return NexusDeployment({ implementation: implementation, factory: factory, bootstrap: bootstrap });
    }

    ////////////////////////////////////////////////////////////////
    //                         Bytecodes                          //
    ////////////////////////////////////////////////////////////////

    // https://etherscan.io/address/0x000000004F43C49e93C970E84001853a70923B03#code
    bytes private constant NEXUS_IMPLEMENTATION_V1_2_0 =
        hex"";

    // https://etherscan.io/address/0x000000001D1D5004a02bAfAb9de2D6CE5b7B13de#code
    bytes private constant NEXUS_ACCOUNT_FACTORY =
        hex"";

    // https://etherscan.io/address/0x00000000D3254452a909E4eeD47455Af7E27C289#code
    bytes private constant NEXUS_BOOTSTRAP_V1_2_0 =
        hex"";
}
